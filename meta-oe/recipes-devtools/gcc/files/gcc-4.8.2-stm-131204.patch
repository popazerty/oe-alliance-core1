diff -ruN gcc-4.8.2/gcc/testsuite/gcc.target/sh/fpchg1.c gcc-st40-4.8.2.131204/gcc/testsuite/gcc.target/sh/fpchg1.c
--- gcc-4.8.2/gcc/testsuite/gcc.target/sh/fpchg1.c	1970-01-01 01:00:00.000000000 +0100
+++ gcc-st40-4.8.2.131204/gcc/testsuite/gcc.target/sh/fpchg1.c	2013-05-28 13:15:34.000000000 +0200
@@ -0,0 +1,24 @@
+/* { dg-do run } */
+/* { dg-options "-O1 -m4-300" } */
+
+/* Check that the fpchg instruction is not moved in a delay slot if the
+   fallthru block uses the mode.  */
+
+__attribute__ ((weak))
+void barrier(void)
+{
+}
+
+float f;
+int i;
+double d;
+
+int main()
+{
+  i = 4;
+
+  barrier();
+
+  i = (f + (i && f && d));
+  return i;
+}
